<?xml version="1.0"?>
<config>
    <modules>
        <Web_Ppfix>
            <version>0.1.0</version>
        </Web_Ppfix>
    </modules>
    <global>
        <models>
            <web_ppfix>
                <class>Web_Ppfix_Model</class>
            </web_ppfix>
            <paypal>
                <rewrite>
                    <cart>Web_Ppfix_Model_Cart</cart>
                    <standard>Web_Ppfix_Model_Standard</standard>
                    <config>Web_Ppfix_Model_Config</config>
                </rewrite>
            </paypal>
        </models>
        <blocks>
            <ppfix>
                <class>Web_Ppfix_Block</class>
            </ppfix>
        </blocks>
        <helpers>
            <ppfix>
                <class>Web_Ppfix_Helper</class>
            </ppfix>
        </helpers>
        <resources>
            <ppfix_setup>
                <setup>
                    <module>Web_Ppfix</module>
                </setup>
            </ppfix_setup>
        </resources>
    </global>
    <adminhtml>
        <events>
            <adminhtml_init_system_config>
                <observers>
                    <ppfix>
                        <class>Web_Ppfix_Model_Observer</class>
                        <method>setConfig</method>
                    </ppfix>
                </observers>
            </adminhtml_init_system_config>
        </events>
    </adminhtml>
</config>